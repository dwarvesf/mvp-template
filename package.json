{
  "name": "mvp-template",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "pnpm --parallel -r --filter \"./apps/*\" dev",
    "build": "pnpm -r --filter ./apps/** run build",
    "start": "pnpm --filter ./apps/api start & pnpm --filter ./apps/web start",
    "lint": "eslint .",
    "format": "prettier -w .",
    "typecheck": "tsc -b --pretty false",
    "test": "pnpm -r run test",
    "codegen": "pnpm --filter @mvp-template/api-client codegen",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.18.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.1",
    "eslint": "^9.8.0",
    "eslint-config-next": "14.2.32",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.14.0",
    "@react-email/components": "^0.5.1",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "jsonwebtoken": "^9.0.2",
    "next-auth": "^4.24.11",
    "orval": "^7.11.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.14.0",
    "react-email": "^4.2.8",
    "react-hook-form": "^7.62.0",
    "resend": "^6.0.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.3"
  }
}
